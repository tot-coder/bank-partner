# Array Banking API - Development Environment Variables

# Application Settings
APP_ENV=development
APP_PORT=8080
LOG_LEVEL=debug

# Database Configuration
DB_HOST=postgres
DB_PORT=5432
DB_USER=arraybank
DB_PASSWORD=arraybank_dev_password
DB_NAME=arraybank_dev
DB_SSLMODE=disable

# Database Migration Settings
AUTO_MIGRATE=true
SEED_DATABASE=true

# JWT Configuration
# For development: Use these keys to persist tokens across service restarts
# For production: Generate new keys with: openssl genrsa -out private.pem 2048 && openssl rsa -in private.pem -pubout -out public.pem
# Then base64 encode them: base64 -w 0 private.pem && base64 -w 0 public.pem
JWT_PRIVATE_
JWT_PUBLIC_
JWT_ISSUER=banking-api
JWT_ACCESS_TOKEN_DURATION=24h
JWT_REFRESH_TOKEN_DURATION=168h

# Server Configuration
SERVER_READ_TIMEOUT=30s
SERVER_WRITE_TIMEOUT=30s
SERVER_IDLE_TIMEOUT=120s

# CORS Configuration
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080

# Rate Limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_REQUESTS_PER_SECOND=10
RATE_LIMIT_BURST=20

# Development Tools
ENABLE_SWAGGER=true
ENABLE_PROFILING=false
